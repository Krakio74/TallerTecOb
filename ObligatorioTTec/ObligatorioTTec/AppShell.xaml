<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ObligatorioTTec.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ObligatorioTTec"
    Title="ObligatorioTTec">

    <FlyoutItem Title="Inicio">
    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

        <ShellContent
        Title="Perfil"
        ContentTemplate="{DataTemplate local:PerfilCliente}"
        Route="PerfilCliente" />
    </FlyoutItem>

    <FlyoutItem Title="Registrarse">
        <ShellContent
        Title="Registrarse"
        ContentTemplate="{DataTemplate local:RegistrarCliente}"
        Route="RegistrarCliente" />
    </FlyoutItem>

    <FlyoutItem Title="Catalogo">
        <ShellContent
     Title="Catalogo"
     ContentTemplate="{DataTemplate local:Catalogo}"
     Route="Catalogo" />
    </FlyoutItem>

</Shell>
